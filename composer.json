{
    "name": "webpress/framework",
    "description": "A Package With All Webpress Package",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Framework\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Framework\\Test": "tests/"
        }
    },
    "require": {
        "webpress/analytics": "^3.0",
        "webpress/category-manager": "^3.0",
        "webpress/comment-manager": "^3.0",
        "webpress/configuration": "^3.0",
        "webpress/contact-manager": "^3.0",
        "webpress/core": "^3.0",
        "webpress/dynamic": "^3.0",
        "webpress/media-manager": "^3.0",
        "webpress/menu-manager": "^3.0",
        "webpress/order-manager": "^3.0",
        "webpress/payment": "^3.0",
        "webpress/post-manager": "^3.0",
        "webpress/product-manager": "^3.0",
        "webpress/script-manager": "^3.0",
        "webpress/search": "^3.0",
        "webpress/seo": "^3.0",
        "webpress/sitemap": "^3.0",
        "webpress/tag-manager": "^3.0",
        "webpress/user-manager": "^3.0",
        "webpress/utility": "^3.0",
        "webpress/view-model": "^3.0",
        "webpress/site-protection": "^3.0",
        "webpress/redirector": "^3.0",
        "webpress/static-file": "^3.0",
        "webpress/export-excel": "^3.0",
        "webpress/language": "^3.0",
        "webpress/dynamic-contact-form": "^3.0",
        "webpress/email-config": "^3.0",
        "webpress/notification": "^3.0",
        "webpress/review": "^3.0",
        "webpress/promotion": "^3.0"
    },
    "require-dev": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}
